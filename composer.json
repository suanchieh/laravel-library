{
    "name": "tommyys/laravel_template",
    "description": "Default laravel template for Axstarzy Dot Com that includes the most used libraries and plugins",
    "authors": [
        {
            "name": "Tommy Lee",
            "email": "tommy.lee@axstarzy.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravelcollective/html": "^5.4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Collective\\Html\\HtmlServiceProvider",
                "Intervention\\Image\\ImageServiceProvider",
                "Axstarzy\\LaravelTemplate\\LaravelTemplateServiceProvider"
            ],
            "aliases": {
                "Form": "Collective\\Html\\FormFacade",
                "Html": "Collective\\Html\\HtmlFacade",
                "Image": "Intervention\\Image\\Facades\\Image"
            }
        }
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Axstarzy\\LaravelTemplate\\": "src/"
        }
    }
}
