{
    "name": "tommyys/laravel_template",
    "description": "Default Laravel template for Axstarzy Dot Com that includes the most used libraries, plugins, helper classes, and migrations",
    "authors": [
        {
            "name": "Tommy Lee",
            "email": "tommy.lee@axstarzy.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravelcollective/html": "^5.4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axstarzy\\LaravelTemplate\\LaravelTemplateServiceProvider"
            ]
        }
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Axstarzy\\LaravelTemplate\\": "src/"
        },
        "files": [
            "Helper.php"
        ]
    }
}
